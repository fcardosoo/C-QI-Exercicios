#include <stdio.h>
int main(){
    int contador; // declarando o contador
    contador = 0; // atribuindo o valor ZERO ao contador
    contador = contador + 1;
    printf("%d\n",contador);
    contador = contador + 1;
    printf("%d\n",contador);
    contador = contador + 1;
    printf("%d\n",contador);
    contador = contador + 1;
    printf("%d\n",contador);
    contador = contador + 1;
    printf("%d\n",contador);
    return 0;
}
