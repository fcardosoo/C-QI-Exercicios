#include <stdio.h>
int main(){
    int numero;
    printf("Digite um numero: ");
    scanf("%d",&numero);
    if(numero>0){ //teste lógico
        printf(" O número é maior que 0\n"); //instruções que serão realizadas
    }
    else{
        printf("O número é menor ou igual a zero\n");
    }
    return 0;
}
